  <node componentName="tJava" componentVersion="0.101" offsetLabelX="0" offsetLabelY="0" posX="96" posY="224">
    <elementParameter field="TEXT" name="UNIQUE_NAME" value="tJava_1" show="false"/>
    <elementParameter field="MEMO_JAVA" name="CODE" value="//vérifie la date&#xA;int annee = -1;&#xA;String mois = &quot;&quot;;&#xA;&#xA;&#xA;try {&#xA;&#x9;annee =  projetBi.getYearFromFile(context.nom_fichier);&#xA;}&#xA;catch( Exception ex) {&#xA;//le dernier paramètre n'est pas un entier&#xA;throw new Exception(&quot;L'année dans le nom de fichier '&quot;+  context.nom_fichier +&quot;' n'est pas valide.&quot;);&#xA;}&#xA;&#xA;//l'annee n'est pas sur 4 chiffres&#xA;if (annee &lt; 1000) {&#xA;throw new Exception(&quot;L'année dans le nom de fichier '&quot;+  context.nom_fichier +&quot;' n'est pas valide. format attendu : yyyy.&quot;);&#xA;}&#xA;&#xA;//ce n'est pas l'année courante&#xA;if ( annee != projetBi.getCurrentYear()) throw new Exception(&quot;L'année dans le nom de fichier '&quot;+  context.nom_fichier +&quot;' n'est pas l'année courante.&quot;);&#xA;&#xA;&#xA;&#xA;if (context.verifier_mois) {&#xA;&#x9;&#xA;&#x9;try {&#xA;&#x9;&#x9;mois = projetBi.getMonthFromFile(context.nom_fichier);&#xA;&#x9;}&#xA;&#x9;catch(Exception ex) {&#xA;&#x9;&#x9;throw new Exception(&quot;Impossible d'extraire le mois dans le nom de fichier '&quot;+  context.nom_fichier +&quot;'.&quot;);&#x9;&#xA;&#x9;}&#xA;&#xA;&#x9;if (!projetBi.getNormalizedMonthList().contains(mois)) {&#xA;&#x9;&#x9;throw new Exception(&quot;Le mois dans le nom de fichier '&quot;+  context.nom_fichier +&quot;' est absent : attendu avec la première lettre en majuscule et sans accent.&quot;);&#x9;&#xA;&#x9;}&#xA;}&#xA;"/>
    <elementParameter field="MEMO_IMPORT" name="IMPORT" value="//import java.util.List;&#xA;import java.lang.Exception;"/>
    <elementParameter field="TEXT" name="LABEL" value="vérification de l'année"/>
    <elementParameter field="TEXT" name="CONNECTION_FORMAT" value="row"/>
